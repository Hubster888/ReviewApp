<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#981313"
    android:foregroundTint="#8E2929"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="419dp"
        android:layout_height="60dp"
        android:background="@color/teal_700"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/imageButton"
        style="@android:style/Widget.DeviceDefault.ImageButton"
        android:layout_width="54dp"
        android:layout_height="11dp"
        android:backgroundTint="#018786"
        android:rotation="90"
        android:rotationY="0"
        android:scaleX="2"
        android:scaleY="2"
        app:srcCompat="@android:drawable/arrow_down_float"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="25dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="426dp"
        android:layout_height="183dp"
        app:cardCornerRadius="10dp"
        tools:layout_editor_absoluteX="-5dp"
        tools:layout_editor_absoluteY="58dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="193dp"
            android:background="@drawable/burger_king_pic"
            android:elevation="20dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|left"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"
                android:elevation="20dp"
                android:fontFamily="sans-serif-condensed-medium"
                android:text="Burger King"
                android:textColor="#FFFFFF"
                android:textSize="40sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton1"
                style="@android:style/Widget.DeviceDefault.ImageButton"
                android:layout_width="99dp"
                android:layout_height="77dp"
                android:layout_gravity="bottom|right"
                android:backgroundTint="#00B81717"
                android:foregroundTint="#FF0000"
                android:scaleX="2"
                android:scaleY="2"
                app:srcCompat="@android:drawable/ic_menu_sort_by_size"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="5dp" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="80dp"
                android:layout_height="64dp"
                android:layout_gravity="right|top"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleX="1.7"
                android:scaleY="1.7"
                app:srcCompat="@android:drawable/ic_menu_save" />

        </FrameLayout>
    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="488dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="242dp">

        <Space
            android:layout_width="match_parent"
            android:layout_height="35dp" />

        <androidx.cardview.widget.CardView
            android:layout_width="357dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            android:elevation="15dp"
            app:cardBackgroundColor="#90018786">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#90018786"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:elevation="5dp"
                        android:scaleX="1.25"
                        android:scaleY="1.25"
                        app:srcCompat="@drawable/account__1_" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginTop="12.5dp"
                        android:layout_weight="1"
                        android:text="Other Bobby"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|top"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="15dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_off" />

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_off" />

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/editTextTextMultiLine"
                    android:layout_width="333dp"
                    android:layout_height="66dp"
                    android:layout_gravity="center"
                    android:ems="10"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:text="Ordered food from the machine, my number never appeared on the screen even though I had a receipt."
                    android:textAlignment="viewStart"
                    android:textColor="#000000"
                    android:textSize="14sp" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="41dp">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageButton
                            android:id="@+id/imageButton5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:elevation="15dp"
                            android:translationY="-10dp"
                            app:srcCompat="@android:drawable/ic_dialog_map" />

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>
                </TableLayout>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <Space
            android:layout_width="wrap_content"
            android:layout_height="35dp" />

        <androidx.cardview.widget.CardView
            android:layout_width="359dp"
            android:layout_height="199dp"
            android:layout_gravity="center"
            android:elevation="15dp"
            app:cardBackgroundColor="#90018786">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:elevation="5dp"
                        android:scaleX="1.25"
                        android:scaleY="1.25"
                        app:srcCompat="@drawable/account__1_" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginTop="12.5dp"
                        android:layout_weight="1"
                        android:text="Bobby Jhons"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|top"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="15dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_on" />

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:srcCompat="@android:drawable/btn_star_big_off" />

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/editTextTextMultiLine"
                    android:layout_width="346dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center"
                    android:ems="10"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:text="Ordered food from the machine, my number never appeared on the screen even though I had a receipt. Had to tell the staff and wait another 10 minutes. Staff are great but clearly there's something ..."
                    android:textAlignment="viewStart"
                    android:textColor="#000000"
                    android:textSize="14sp" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="41dp">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <EditText
                            android:id="@+id/editTextNumber2"
                            android:layout_width="22dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginBottom="5dp"
                            android:ems="10"
                            android:inputType="number"
                            android:text="2"
                            android:textAlignment="center"
                            android:textColor="#000000"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <ImageButton
                            android:id="@+id/imageButton5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:elevation="15dp"
                            app:srcCompat="@android:drawable/ic_dialog_map" />

                        <ImageButton
                            android:id="@+id/imageButton6"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_marginBottom="-50dp"
                            android:elevation="15dp"
                            app:srcCompat="@drawable/burger_king_pic" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </TableRow>
                </TableLayout>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <Space
            android:layout_width="wrap_content"
            android:layout_height="20dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="76dp"
            android:layout_height="76dp"
            android:layout_gravity="right|bottom"
            android:background="@drawable/icons8_plus___24"
            android:translationX="-20dp"
            android:translationY="-50dp"
            app:backgroundTint="@color/teal_700" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>